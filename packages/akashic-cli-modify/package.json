{
  "name": "@akashic/akashic-cli-modify",
  "version": "0.2.1",
  "description": "A module to update game.json parameters",
  "main": "lib/index.js",
  "scripts": {
    "prepublish": "npm run clean && npm run build",
    "clean": "shx rm -rf lib && shx rm -rf spec/build",
    "build": "tsc -p ./",
    "lint": "tslint --type-check -c ../tslint.json --project ./tsconfig.json",
    "test": "npm run test:build && npm run test:jest && npm run lint",
    "test:build": "tsc -p spec/",
    "test:jest": "jest"
  },
  "author": "DWANGO Co., Ltd.",
  "license": "MIT",
  "bin": {
    "akashic-cli-modify": "./bin/run"
  },
  "files": [
    "bin",
    "lib",
    "package.json"
  ],
  "devDependencies": {
    "@types/jasmine": "2.8.16",
    "@types/mock-fs": "3.6.30",
    "@types/node": "10.17.6",
    "jest": "24.9.0",
    "mock-fs": "4.10.4",
    "shx": "0.3.2",
    "tslint": "5.20.1",
    "typescript": "2.9.2"
  },
  "typings": "lib/index.d.ts",
  "dependencies": {
    "@akashic/akashic-cli-commons": "0.3.1",
    "commander": "^2.16.0"
  },
  "publishConfig": {
    "access": "public",
    "@akashic:registry": "https://registry.npmjs.org/"
  }
}
