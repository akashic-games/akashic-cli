{
  "name": "@akashic/akashic-cli-modify",
  "version": "0.4.49",
  "description": "A module to update game.json parameters",
  "main": "lib/index.js",
  "scripts": {
    "prepublish": "npm run clean && npm run build",
    "clean": "shx rm -rf lib && shx rm -rf spec/build",
    "build": "tsc -p ./",
    "lint": "tslint --type-check -c ../tslint.json --project ./tsconfig.json",
    "test": "npm run test:build && npm run test:jest && npm run lint",
    "test:build": "tsc -p spec/",
    "test:jest": "jest"
  },
  "author": "DWANGO Co., Ltd.",
  "license": "MIT",
  "bin": {
    "akashic-cli-modify": "./bin/run"
  },
  "files": [
    "bin",
    "lib",
    "package.json"
  ],
  "devDependencies": {
    "@types/jasmine": "3.5.14",
    "@types/mock-fs": "4.10.0",
    "@types/node": "14.11.2",
    "jest": "26.4.2",
    "mock-fs": "4.13.0",
    "shx": "0.3.2",
    "tslint": "6.1.3",
    "typescript": "3.9.7"
  },
  "typings": "lib/index.d.ts",
  "dependencies": {
    "@akashic/akashic-cli-commons": "0.5.37",
    "commander": "^5.0.0"
  },
  "publishConfig": {
    "access": "public",
    "@akashic:registry": "https://registry.npmjs.org/"
  }
}
