{
  "name": "@akashic/akashic-cli-uninstall",
  "version": "0.9.0",
  "description": "npm uninstall wrapper module for Akashic games.",
  "main": "lib/index.js",
  "scripts": {
    "prepublish": "npm run clean && npm run build",
    "clean": "rimraf ./lib && rimraf ./spec/build",
    "start": "npm run build && node bin/run",
    "build": "tsc -p ./",
    "lint": "tslint -c ../tslint.json --project ./tsconfig.json",
    "test": "npm run build && npm run test:compile  && npm run test:jest && npm run lint",
    "test:compile": "tsc -p ./spec",
    "test:jest": "jest"
  },
  "author": "DWANGO Co., Ltd.",
  "license": "MIT",
  "bin": {
    "akashic-cli-uninstall": "./bin/run"
  },
  "files": [
    "bin",
    "lib",
    "README.md",
    "package.json"
  ],
  "devDependencies": {
    "@types/commander": "2.12.2",
    "@types/form-data": "2.2.1",
    "@types/fs-extra": "9.0.8",
    "@types/jasmine": "3.6.6",
    "@types/mock-fs": "4.13.0",
    "@types/node": "14.14.34",
    "jest": "26.6.3",
    "mock-fs": "4.13.0",
    "rimraf": "3.0.2",
    "tslint": "6.1.3",
    "typescript": "3.9.9"
  },
  "dependencies": {
    "@akashic/akashic-cli-commons": "0.8.0",
    "commander": "7.1.0"
  },
  "typings": "lib/index.d.ts",
  "publishConfig": {
    "access": "public",
    "@akashic:registry": "https://registry.npmjs.org/"
  }
}
