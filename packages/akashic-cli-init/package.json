{
  "name": "@akashic/akashic-cli-init",
  "version": "2.0.3",
  "description": "A module to get your Akashic game started.",
  "type": "module",
  "scripts": {
    "prepublish": "npm run clean && npm run build",
    "clean": "rimraf ./lib",
    "start": "npm run build && node bin/run.js",
    "build": "tsc -p ./",
    "lint": "eslint \"./src/**/*.ts\" --fix",
    "test": "npm run test:vitest && npm run lint",
    "test:vitest": "vitest run",
    "prepublishOnly": "node ../../scripts/generateShrinkwrapJson.js"
  },
  "author": "DWANGO Co., Ltd.",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/akashic-games/akashic-cli.git",
    "directory": "packages/akashic-cli-init"
  },
  "bin": {
    "akashic-cli-init": "./bin/run.js"
  },
  "files": [
    "lib",
    "npm-shrinkwrap.json"
  ],
  "devDependencies": {
    "@akashic/eslint-config": "3.0.2",
    "@types/express": "5.0.6",
    "@types/form-data": "2.5.0",
    "@types/fs-extra": "11.0.4",
    "@types/mock-fs": "4.13.4",
    "@types/node": "24.10.13",
    "@types/unzipper": "0.10.10",
    "express": "^5.0.0",
    "get-port": "7.1.0",
    "mock-fs": "5.2.0",
    "rimraf": "6.1.3",
    "typescript": "5.5.4",
    "vitest": "^4.0.0"
  },
  "dependencies": {
    "@akashic/akashic-cli-commons": "1.0.3",
    "@akashic/akashic-cli-extra": "2.0.3",
    "@inquirer/checkbox": "^4.2.2",
    "commander": "12.1.0",
    "fs-extra": "11.2.0",
    "glob": "11.1.0",
    "ignore": "7.0.0",
    "prompt": "1.3.0",
    "unzipper": "0.12.3"
  },
  "publishConfig": {
    "access": "public",
    "@akashic:registry": "https://registry.npmjs.org/"
  }
}
