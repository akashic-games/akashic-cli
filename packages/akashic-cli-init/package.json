{
  "name": "@akashic/akashic-cli-init",
  "version": "0.10.46",
  "description": "A module to get your Akashic game started.",
  "main": "lib/index.js",
  "scripts": {
    "prepublish": "npm run clean && npm run build",
    "clean": "rimraf ./lib && rimraf ./spec/build",
    "start": "npm run build && node bin/run",
    "build": "tsc -p ./ && npm run template && npm run zip && npm run zip-v3",
    "lint": "tslint -c ../tslint.json --project ./tsconfig.json",
    "zip": "npm run zip:js && npm run zip:ts && npm run zip:js-min && npm run zip:ts-min && npm run zip:js-ranking && npm run zip:ts-ranking",
    "zip-v3": "npm run zip:js-v3 && npm run zip:ts-v3 && npm run zip:js-min-v3 && npm run zip:ts-min-v3 && npm run zip:js-ranking-v3 && npm run zip:ts-ranking-v3",
    "zip:js": "cd ./templates/javascript/ && bestzip ../../templates/javascript.zip ./ && cd .. && rimraf ./javascript",
    "zip:js-v3": "cd ./templates/javascript-v3/ && bestzip ../../templates/javascript-v3.zip ./ && cd .. && rimraf ./javascript-v3",
    "zip:js-min": "cd ./templates/javascript-minimal/ && bestzip ../../templates/javascript-minimal.zip ./ && cd .. && rimraf ./javascript-minimal",
    "zip:js-min-v3": "cd ./templates/javascript-minimal-v3/ && bestzip ../../templates/javascript-minimal-v3.zip ./ && cd .. && rimraf ./javascript-minimal-v3",
    "zip:js-ranking": "cd ./templates/javascript-shin-ichiba-ranking/ && bestzip ../../templates/javascript-shin-ichiba-ranking.zip ./ && cd .. && rimraf ./javascript-shin-ichiba-ranking",
    "zip:js-ranking-v3": "cd ./templates/javascript-shin-ichiba-ranking-v3/ && bestzip ../../templates/javascript-shin-ichiba-ranking-v3.zip ./ && cd .. && rimraf ./javascript-shin-ichiba-ranking-v3",
    "zip:ts": "cd ./templates/typescript/ && bestzip ../../templates/typescript.zip ./ && cd .. && rimraf ./typescript",
    "zip:ts-v3": "cd ./templates/typescript-v3/ && bestzip ../../templates/typescript-v3.zip ./ && cd .. && rimraf ./typescript-v3",
    "zip:ts-min": "cd ./templates/typescript-minimal/ && bestzip ../../templates/typescript-minimal.zip ./ && cd .. && rimraf ./typescript-minimal",
    "zip:ts-min-v3": "cd ./templates/typescript-minimal-v3/ && bestzip ../../templates/typescript-minimal-v3.zip ./ && cd .. && rimraf ./typescript-minimal-v3",
    "zip:ts-ranking": "cd ./templates/typescript-shin-ichiba-ranking/ && bestzip ../../templates/typescript-shin-ichiba-ranking.zip ./ && cd .. && rimraf ./typescript-shin-ichiba-ranking",
    "zip:ts-ranking-v3": "cd ./templates/typescript-shin-ichiba-ranking-v3/ && bestzip ../../templates/typescript-shin-ichiba-ranking-v3.zip ./ && cd .. && rimraf ./typescript-shin-ichiba-ranking-v3",
    "template": "node ./build/generateTemplateSource.js",
    "http": "http-server -p 18080 &",
    "test": "npm run test:jest && npm run lint",
    "test:jest": "jest",
    "pretest": "npm run http",
    "posttest": "kill $(lsof -t -i:18080)",
    "update:template-modules": "node ./build/updateModulesOfTemplates.js"
  },
  "author": "DWANGO Co., Ltd.",
  "license": "MIT",
  "bin": {
    "akashic-cli-init": "./bin/run"
  },
  "files": [
    "README.md",
    "bin",
    "lib",
    "templates",
    "package.json"
  ],
  "devDependencies": {
    "@types/commander": "2.12.0",
    "@types/form-data": "2.2.1",
    "@types/fs-extra": "9.0.1",
    "@types/node": "10.17.27",
    "@types/request": "2.48.5",
    "@types/unzipper": "0.10.3",
    "bestzip": "2.1.5",
    "http-server": "0.12.3",
    "jest": "26.1.0",
    "mock-fs": "4.12.0",
    "npm-check-updates": "7.0.2",
    "rimraf": "3.0.2",
    "shelljs": "0.8.4",
    "tslint": "5.20.1",
    "typescript": "3.9.7"
  },
  "typings": "lib/index.d.ts",
  "dependencies": {
    "@akashic/akashic-cli-commons": "0.5.18",
    "@akashic/akashic-cli-config": "0.4.26",
    "commander": "5.1.0",
    "fs-extra": "9.0.1",
    "prompt": "1.0.0",
    "request": "^2.74.0",
    "unzipper": "^0.10.0"
  },
  "publishConfig": {
    "access": "public",
    "@akashic:registry": "https://registry.npmjs.org/"
  }
}
