{
  "name": "@akashic/akashic-cli-init",
  "version": "2.0.0-next.1",
  "description": "A module to get your Akashic game started.",
  "type": "module",
  "scripts": {
    "prepublish": "npm run clean && npm run build",
    "clean": "rimraf ./lib",
    "start": "npm run build && node bin/run.js",
    "build": "tsc -p ./",
    "lint": "eslint -c .eslintrc.cjs --fix \"./src/**/*.ts\"",
    "test": "npm run test:vitest && npm run lint",
    "test:vitest": "vitest run"
  },
  "author": "DWANGO Co., Ltd.",
  "license": "MIT",
  "bin": {
    "akashic-cli-init": "./bin/run.js"
  },
  "files": [
    "lib"
  ],
  "devDependencies": {
    "@akashic/eslint-config": "2.1.0",
    "@types/express": "4.17.21",
    "@types/form-data": "2.5.0",
    "@types/fs-extra": "9.0.13",
    "@types/mock-fs": "4.13.4",
    "@types/node": "20.14.14",
    "@types/unzipper": "0.10.10",
    "@typescript-eslint/eslint-plugin": "6.21.0",
    "eslint": "8.57.0",
    "eslint-plugin-import": "2.29.1",
    "express": "^4.20.0",
    "get-port": "5.1.1",
    "mock-fs": "5.2.0",
    "rimraf": "3.0.2",
    "typescript": "5.5.4",
    "vitest": "^2.1.1"
  },
  "dependencies": {
    "@akashic/akashic-cli-commons": "1.0.0-next.1",
    "@akashic/akashic-cli-extra": "2.0.0-next.1",
    "commander": "12.1.0",
    "fs-extra": "11.2.0",
    "glob": "11.0.0",
    "ignore": "7.0.0",
    "prompt": "1.3.0",
    "unzipper": "0.12.3"
  },
  "publishConfig": {
    "access": "public",
    "@akashic:registry": "https://registry.npmjs.org/"
  }
}
