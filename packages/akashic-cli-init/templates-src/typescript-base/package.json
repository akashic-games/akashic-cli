    "@akashic/akashic-sandbox": "~0.17.35",
    "@akashic/headless-akashic": "^2.1.0",
    "@types/jest": "26.0.24",
    "@types/node": "14.17.21",
    "@typescript-eslint/eslint-plugin": "3.10.1",
    "@typescript-eslint/parser": "3.10.1",
    "eslint": "7.32.0",
    "eslint-plugin-import": "2.24.2",
    "jest": "26.6.3",
    "rimraf": "3.0.2",
    "ts-jest": "26.5.6",
    "typescript": "3.9.10"
  }
