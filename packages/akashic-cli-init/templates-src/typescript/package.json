    "test": "jest"
  },
  "author": "",
  "license": "",
  "devDependencies": {
    "@akashic/akashic-cli-export-html": "~0.2.9",
    "@akashic/akashic-cli-export-zip": "~0.3.5",
    "@akashic/akashic-cli-scan": "~0.2.6",
    "@akashic/akashic-engine": "~2.4.11",
    "@akashic/akashic-sandbox": "~0.13.55",
    "jest": "~24.5.0",
    "rimraf": "~2.6.2",
    "tslint": "~5.8.0",
    "typescript": "~2.6.2"
  }
}
