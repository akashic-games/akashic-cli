    "@akashic/akashic-engine": "~3.0.0-beta.27",
    "@akashic/akashic-sandbox": "~0.16.20",
    "@types/jest": "26.0.14",
    "@types/node": "14.11.1",
    "@typescript-eslint/eslint-plugin": "3.10.1",
    "@typescript-eslint/parser": "3.10.1",
    "eslint": "7.9.0",
    "eslint-plugin-import": "2.22.0",
    "jest": "26.4.2",
