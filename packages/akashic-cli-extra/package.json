{
  "name": "@akashic/akashic-cli-extra",
  "version": "1.2.3",
  "description": "Extra tools of akashic-cli-xxx",
  "bin": {
    "akashic-cli-config": "./bin/akashic-cli-config",
    "akashic-cli-modify": "./bin/akashic-cli-modify",
    "akashic-cli-stat": "./bin/akashic-cli-stat"
  },
  "scripts": {
    "prepublish": "npm run clean && npm run build",
    "clean": "rimraf ./lib && rimraf ./spec/build",
    "start": "npm run build && node bin/run",
    "build": "tsc -p ./",
    "test": "npm run test:build && npm run test:jest && npm run lint",
    "lint": "eslint \"src/**/*.ts\" \"spec/src/**/*.ts\" --fix",
    "test:build": "tsc -p spec/",
    "test:jest": "jest"
  },
  "author": "DWANGO Co., Ltd.",
  "license": "MIT",
  "files": [
    "README.md",
    "package.json",
    "bin",
    "lib"
  ],
  "dependencies": {
    "@akashic/akashic-cli-commons": "^0.7.0",
    "commander": "8.0.0",
    "ini": "2.0.0",
    "lodash.get": "4.4.2",
    "lodash.set": "4.3.2",
    "lodash.unset": "4.5.2"
  },
  "devDependencies": {
    "@akashic/eslint-config": "0.1.2",
    "@types/commander": "2.12.0",
    "@types/ini": "1.3.30",
    "@types/jasmine": "3.7.8",
    "@types/mock-fs": "4.13.1",
    "@types/node": "14.17.4",
    "@typescript-eslint/eslint-plugin": "4.28.2",
    "eslint": "7.22.0",
    "eslint-plugin-import": "2.23.4",
    "eslint-plugin-jest": "24.3.6",
    "jest": "26.6.3",
    "mock-fs": "4.14.0",
    "rimraf": "3.0.2",
    "typescript": "3.9.10"
  },
  "publishConfig": {
    "access": "public",
    "@akashic:registry": "https://registry.npmjs.org/"
  }
}
