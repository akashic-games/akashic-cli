{
  "name": "@akashic/akashic-cli-extra",
  "version": "1.7.8",
  "description": "Extra tools of akashic-cli-xxx",
  "type": "module",
  "types": "./lib/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./lib/index.d.ts",
        "default": "./lib/index.js"
      }
    },
    "./lib/*": {
      "import": {
        "types": "./lib/*",
        "default": "./lib/*"
      }
    },
    "./package.json": "./package.json"
  },
  "bin": {
    "akashic-cli-config": "./bin/akashic-cli-config",
    "akashic-cli-modify": "./bin/akashic-cli-modify",
    "akashic-cli-stat": "./bin/akashic-cli-stat"
  },
  "scripts": {
    "prepublish": "npm run clean && npm run build",
    "clean": "rimraf ./lib && rimraf ./spec/build",
    "start": "npm run build && node bin/run",
    "build": "tsc -p ./",
    "test": "npm run test:vitest && npm run test:lint",
    "test:vitest": "vitest run",
    "test:lint": "eslint -c .eslintrc.cjs --fix"
  },
  "author": "DWANGO Co., Ltd.",
  "license": "MIT",
  "files": [
    "lib"
  ],
  "dependencies": {
    "@akashic/akashic-cli-commons": "0.15.3",
    "commander": "12.1.0",
    "ini": "5.0.0",
    "lodash": "4.17.21"
  },
  "devDependencies": {
    "@akashic/eslint-config": "2.1.0",
    "@types/commander": "2.12.2",
    "@types/ini": "1.3.34",
    "@types/lodash": "4.17.9",
    "@types/mock-fs": "4.13.4",
    "@types/node": "14.18.63",
    "@typescript-eslint/eslint-plugin": "6.21.0",
    "eslint": "8.57.0",
    "eslint-plugin-import": "2.29.1",
    "mock-fs": "5.2.0",
    "rimraf": "3.0.2",
    "typescript": "5.5.4",
    "vitest": "^2.0.5"
  },
  "publishConfig": {
    "access": "public",
    "@akashic:registry": "https://registry.npmjs.org/"
  }
}
