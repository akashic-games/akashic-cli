{
  "name": "@akashic/akashic-cli-export-zip",
  "version": "0.3.12",
  "description": "A module to export akashic game as a zip",
  "main": "lib/index.js",
  "scripts": {
    "prepublish": "npm run clean && npm run build",
    "clean": "rimraf ./lib && rimraf ./spec/build",
    "start": "npm run build && node bin/run",
    "build": "tsc -p ./",
    "lint": "npm run lint:src && npm run lint:spec",
    "lint:src": "tslint --project tsconfig.json -c ../tslint.json",
    "lint:spec": "tslint --project ./spec/tsconfig.json -c ../tslint.json",
    "test": "npm run test:ts && npm run lint",
    "test:ts": "npm run test:ts:compile && npm run test:ts:jasmine",
    "test:ts:compile": "npm run build && tsc -p ./spec",
    "test:ts:jasmine": "istanbul cover --report text --report html --colors -i ./lib/main.node.js ./node_modules/jasmine/bin/jasmine.js"
  },
  "author": "DWANGO Co., Ltd.",
  "license": "MIT",
  "bin": {
    "akashic-cli-export-zip": "./bin/run"
  },
  "files": [
    "README.md",
    "bin",
    "lib",
    "templates",
    "package.json"
  ],
  "devDependencies": {
    "@types/archiver": "~2.1.2",
    "@types/browserify": "~12.0.33",
    "@types/commander": "~2.3.31",
    "@types/fs-extra": "^5.0.0",
    "@types/fs-readdir-recursive": "~1.0.0",
    "@types/jasmine": "~2.8.2",
    "@types/mock-fs": "~3.6.30",
    "@types/node": "~8.0.53",
    "@types/uglify-js": "2.6.30",
    "istanbul": "^0.4.5",
    "jasmine": "^2.1.1",
    "jasmine-reporters": "~2.0.4",
    "mock-fs": "~4.8.0",
    "rimraf": "^2.6.1",
    "tslint": "~5.4.3",
    "typescript": "~2.8.0"
  },
  "typings": "lib/index.d.ts",
  "dependencies": {
    "@akashic/akashic-cli-commons": "0.2.22",
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.3.1",
    "archiver": "3.0.0",
    "babel-preset-env": "^1.7.0",
    "browserify": "~16.2.3",
    "commander": "2.8.1",
    "fs-extra": "^3.0.1",
    "fs-readdir-recursive": "~0.1.2",
    "uglify-js": "2.6.1"
  },
  "publishConfig": {
    "access": "public",
    "@akashic:registry": "https://registry.npmjs.org/"
  }
}
