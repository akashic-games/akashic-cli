{
  "name": "@akashic/akashic-cli-install",
  "version": "0.6.57",
  "description": "npm install wrapper module for Akashic games",
  "main": "lib/index.js",
  "scripts": {
    "prepublish": "npm run clean && npm run build",
    "clean": "rimraf ./lib && rimraf ./spec/build",
    "start": "npm run build && node bin/run",
    "build": "tsc -p ./",
    "lint": "tslint -c ../tslint.json --project ./tsconfig.json",
    "test": "npm run test:jest && npm run lint",
    "test:jest": "jest"
  },
  "author": "DWANGO Co., Ltd.",
  "license": "MIT",
  "bin": {
    "akashic-cli-install": "./bin/run"
  },
  "files": [
    "bin",
    "lib",
    "README.md",
    "package.json"
  ],
  "devDependencies": {
    "@types/commander": "2.12.2",
    "@types/jasmine": "3.5.14",
    "@types/node": "14.11.2",
    "@types/tar": "4.0.3",
    "jest": "26.5.2",
    "mock-fs": "4.13.0",
    "rimraf": "3.0.2",
    "tslint": "6.1.3",
    "typescript": "3.9.7"
  },
  "dependencies": {
    "@akashic/akashic-cli-commons": "0.5.41",
    "commander": "5.1.0",
    "tar": "6.0.5"
  },
  "typings": "lib/index.d.ts",
  "publishConfig": {
    "access": "public",
    "@akashic:registry": "https://registry.npmjs.org/"
  }
}
