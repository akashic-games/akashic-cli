{
  "name": "@akashic/akashic-cli-install",
  "version": "0.6.3",
  "description": "npm install wrapper module for Akashic games",
  "main": "lib/index.js",
  "scripts": {
    "prepublish": "npm run clean && npm run build",
    "clean": "rimraf ./lib && rimraf ./spec/build",
    "start": "npm run build && node bin/run",
    "build": "tsc -p ./",
    "lint": "tslint -c ../tslint.json --project ./tsconfig.json",
    "test": "npm run test:jest && npm run lint",
    "test:jest": "jest"
  },
  "author": "DWANGO Co., Ltd.",
  "license": "MIT",
  "bin": {
    "akashic-cli-install": "./bin/run"
  },
  "files": [
    "bin",
    "lib",
    "README.md",
    "package.json"
  ],
  "devDependencies": {
    "@types/commander": "2.12.2",
    "@types/jasmine": "2.8.16",
    "@types/node": "10.17.19",
    "jest": "24.9.0",
    "mock-fs": "4.11.0",
    "rimraf": "2.7.1",
    "tslint": "5.20.1",
    "typescript": "3.8.3"
  },
  "dependencies": {
    "@akashic/akashic-cli-commons": "0.5.3",
    "commander": "2.20.3"
  },
  "typings": "lib/index.d.ts",
  "publishConfig": {
    "access": "public",
    "@akashic:registry": "https://registry.npmjs.org/"
  }
}
