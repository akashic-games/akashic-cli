{
  "name": "@akashic/akashic-cli-serve",
  "version": "1.16.11",
  "description": "A development server for Akashic Engine",
  "scripts": {
    "prepare": "npm run setup",
    "prepublish": "npm run build",
    "setup": "npm run copy:material-icons && npm run copy:agv && npm run copy:engine-files",
    "copy:material-icons": "node build/copyIcons.js",
    "copy:agv": "node build/copyAgv.js",
    "copy:engine-files": "node build/updateEngineFiles.js",
    "watch:client": "webpack --mode development --config ./src/client/webpack.config.js --watch",
    "build": "npm run build:client && npm run build:server && shx cp ./config/engineFilesVersion.json ./lib/server",
    "build:client": "webpack --config ./src/client/webpack.config.js && tsc -p ./src/client/tsconfig.module.json",
    "build:server": "tsc -p ./src/server",
    "test": "jest && npm run lint",
    "lint": "npm run lint:client && npm run lint:server",
    "lint:client": "eslint -c ./src/client/.eslintrc.js \"src/client/**/*.ts\" \"src/common/**/*.ts\" \"src/client/**/*.tsx\" --fix",
    "lint:server": "eslint \"src/server/**/*.ts\" --fix",
    "storybook": "start-storybook -p 9001 -c .storybook -s ./www"
  },
  "author": "DWANGO Co., Ltd.",
  "license": "MIT",
  "bin": {
    "akashic-cli-serve": "./bin/run"
  },
  "dependencies": {
    "@akashic/akashic-cli-commons": "0.15.3",
    "@akashic/akashic-cli-scan": "0.17.6",
    "@akashic/amflow-util": "^1.3.0",
    "@akashic/game-configuration": "^2.1.0",
    "@akashic/headless-driver": "2.15.6",
    "@akashic/sandbox-configuration": "^2.3.0",
    "@akashic/trigger": "2.1.1",
    "@msgpack/msgpack": "2.8.0",
    "chalk": "4.1.2",
    "chokidar": "3.6.0",
    "commander": "12.1.0",
    "component-emitter": "2.0.0",
    "cors": "2.8.5",
    "ejs": "3.1.10",
    "express": "4.19.2",
    "lodash": "4.17.21",
    "open": "8.4.2",
    "query-string": "6.14.1",
    "rxjs": "7.8.1",
    "socket.io": "4.7.5",
    "socket.io-parser": "4.2.3"
  },
  "devDependencies": {
    "@akashic/agvw": "^1.0.4",
    "@akashic/amflow": "~3.3.0",
    "@akashic/eslint-config": "1.1.1",
    "@akashic/pdi-types": "~1.14.0",
    "@akashic/playlog": "~3.3.0",
    "@storybook/addon-actions": "^6.1.21",
    "@storybook/addon-knobs": "^6.1.21",
    "@storybook/react": "^6.1.21",
    "@types/express": "4.17.21",
    "@types/express-serve-static-core": "4.19.5",
    "@types/node": "22.2.0",
    "@types/query-string": "6.3.0",
    "@types/react": "16.14.60",
    "@types/react-dom": "16.9.24",
    "@types/react-measure": "2.0.12",
    "@types/storybook__addon-actions": "3.4.3",
    "@types/storybook__react": "4.0.2",
    "@typescript-eslint/eslint-plugin": "5.62.0",
    "browserify": "16.5.2",
    "css-loader": "4.3.0",
    "es6-promise": "4.2.8",
    "eslint": "8.57.0",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-jest": "26.9.0",
    "eslint-plugin-react": "7.35.0",
    "get-port": "5.1.1",
    "jest": "29.7.0",
    "material-icons": "1.13.12",
    "mobx": "5.15.7",
    "mobx-react": "6.3.1",
    "react": "16.14.0",
    "react-dom": "16.14.0",
    "react-measure": "2.5.2",
    "rimraf": "3.0.2",
    "scroll-into-view-if-needed": "2.2.31",
    "shx": "0.3.4",
    "socket.io-client": "3.1.3",
    "storybook-addon-jsx": "7.3.14",
    "style-loader": "1.3.0",
    "ts-loader": "8.4.0",
    "typescript": "4.9.5",
    "uglify-js": "3.19.2",
    "uglify-save-license": "0.4.1",
    "webpack": "5.93.0",
    "webpack-cli": "5.1.4",
    "whatwg-fetch": "3.6.20"
  },
  "overrides": {
    "@types/mime": "4.0.0"
  },
  "files": [
    "package.json",
    "README.md",
    "bin",
    "lib",
    "views",
    "www/public"
  ],
  "publishConfig": {
    "access": "public",
    "@akashic:registry": "https://registry.npmjs.org/"
  }
}
