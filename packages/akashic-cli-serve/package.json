{
  "name": "@akashic/akashic-cli-serve",
  "version": "1.9.17",
  "description": "A development server for Akashic Engine",
  "scripts": {
    "prepublish": "npm run setup && npm run build",
    "setup": "npm run copy:material-icons",
    "copy:material-icons": "shx mkdir -p www/public/thirdparty && shx cp -r node_modules/material-design-icons/iconfont www/public/thirdparty/material-design-icons",
    "copy:agv": "node build/AkashicGameViewWebBuilder.js && shx mkdir -p www/internal/untrusted_loader",
    "copy:engine-files": "node build/updateEngineFiles.js",
    "watch:client": "webpack --mode development --config ./src/client/webpack.config.js --watch",
    "build": "npm run build:client && npm run build:server && shx cp ./config/engineFilesVersion.json ./lib/server",
    "build:client": "webpack --config ./src/client/webpack.config.js",
    "build:server": "tsc -p ./src/server",
    "test": "jest && npm run lint",
    "lint": "npm run lint:client && npm run lint:server",
    "lint:client": "eslint -c ./src/client/.eslintrc.js \"src/client/**/*.ts\" \"src/common/**/*.ts\" --fix",
    "lint:server": "eslint \"src/server/**/*.ts\" --fix",
    "storybook": "start-storybook -p 9001 -c .storybook -s ./www"
  },
  "author": "DWANGO Co., Ltd.",
  "license": "MIT",
  "bin": {
    "akashic-cli-serve": "./bin/run"
  },
  "dependencies": {
    "@akashic/akashic-cli-commons": "0.8.10",
    "@akashic/akashic-cli-scan": "0.8.0",
    "@akashic/headless-driver": "1.9.3",
    "@akashic/trigger": "1.0.0",
    "@msgpack/msgpack": "2.7.0",
    "chalk": "4.1.2",
    "chokidar": "3.5.2",
    "commander": "8.1.0",
    "component-emitter": "1.3.0",
    "ejs": "3.1.6",
    "express": "4.17.1",
    "lodash": "4.17.21",
    "open": "8.2.1",
    "rxjs": "6.6.7",
    "socket.io": "4.1.3",
    "socket.io-parser": "4.0.4"
  },
  "devDependencies": {
    "@akashic/amflow": "~3.1.0",
    "@akashic/eslint-config": "0.1.2",
    "@akashic/pdi-types": "~1.2.0",
    "@akashic/playlog": "~3.1.0",
    "@storybook/addon-actions": "^6.1.21",
    "@storybook/addon-knobs": "^6.1.21",
    "@storybook/react": "^6.1.21",
    "@types/express": "4.17.13",
    "@types/express-serve-static-core": "4.17.24",
    "@types/node": "14.17.12",
    "@types/query-string": "6.2.0",
    "@types/react": "16.14.14",
    "@types/react-dom": "16.9.14",
    "@types/storybook__addon-actions": "3.4.3",
    "@types/storybook__react": "4.0.2",
    "@typescript-eslint/eslint-plugin": "4.29.3",
    "browserify": "16.5.2",
    "css-loader": "4.3.0",
    "es6-promise": "4.2.8",
    "eslint": "7.32.0",
    "eslint-plugin-import": "2.24.2",
    "eslint-plugin-jest": "24.4.0",
    "get-port": "5.1.1",
    "jest": "26.6.3",
    "material-design-icons": "3.0.1",
    "mobx": "5.15.7",
    "mobx-react": "6.3.1",
    "node-fetch": "2.6.1",
    "query-string": "6.14.1",
    "react": "16.14.0",
    "react-dom": "16.14.0",
    "rimraf": "3.0.2",
    "scroll-into-view-if-needed": "2.2.28",
    "shx": "0.3.3",
    "socket.io-client": "3.1.3",
    "storybook-addon-jsx": "7.3.13",
    "style-loader": "1.3.0",
    "ts-loader": "8.3.0",
    "typescript": "4.4.2",
    "uglify-js": "3.14.1",
    "uglify-save-license": "0.4.1",
    "webpack": "4.46.0",
    "webpack-cli": "3.3.12",
    "whatwg-fetch": "3.6.2"
  },
  "optionalDependencies": {
    "@akashic/akashic-gameview-web": "3.0.3"
  },
  "files": [
    "package.json",
    "README.md",
    "bin",
    "lib",
    "views",
    "www/public"
  ],
  "publishConfig": {
    "access": "public",
    "@akashic:registry": "https://registry.npmjs.org/"
  }
}
