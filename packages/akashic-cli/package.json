{
  "name": "@akashic/akashic-cli",
  "version": "2.4.0",
  "description": "Command-line utility for akashic game development",
  "bin": {
    "akashic": "./bin/akashic"
  },
  "scripts": {
    "prepublish": "npm run clean && npm run build",
    "clean": "rimraf ./lib && rimraf ./spec/build",
    "start": "npm run build && node bin/run",
    "build": "tsc -p ./",
    "lint": "eslint -c .eslintrc src/**/*.ts --fix",
    "test": "npm run lint"
  },
  "author": "DWANGO Co., Ltd.",
  "license": "MIT",
  "files": [
    "README.md",
    "package.json",
    "bin",
    "lib"
  ],
  "dependencies": {
    "@akashic/akashic-cli-commons": "0.7.0",
    "@akashic/akashic-cli-config": "0.6.0",
    "@akashic/akashic-cli-export-html": "1.3.0",
    "@akashic/akashic-cli-export-zip": "0.8.0",
    "@akashic/akashic-cli-init": "1.3.0",
    "@akashic/akashic-cli-install": "0.8.0",
    "@akashic/akashic-cli-modify": "0.6.0",
    "@akashic/akashic-cli-scan": "0.7.0",
    "@akashic/akashic-cli-serve": "1.4.0",
    "@akashic/akashic-cli-stat": "0.6.0",
    "@akashic/akashic-cli-uninstall": "0.8.0",
    "@akashic/akashic-cli-update": "0.7.0",
    "commander": "5.1.0"
  },
  "devDependencies": {
    "@akashic/eslint-config": "0.1.2",
    "@types/commander": "2.12.0",
    "@types/node": "14.14.20",
    "@typescript-eslint/eslint-plugin": "4.14.0",
    "eslint": "7.18.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jest": "24.1.3",
    "rimraf": "3.0.2",
    "typescript": "3.9.7"
  },
  "publishConfig": {
    "access": "public",
    "@akashic:registry": "https://registry.npmjs.org/"
  }
}
