{
  "name": "@akashic/akashic-cli",
  "version": "2.6.6",
  "description": "Command-line utility for akashic game development",
  "bin": {
    "akashic": "./bin/akashic"
  },
  "scripts": {
    "prepublish": "npm run clean && npm run build",
    "clean": "rimraf ./lib && rimraf ./spec/build",
    "start": "npm run build && node bin/run",
    "build": "tsc -p ./",
    "lint": "tslint -c ../tslint.json --project ./tsconfig.json",
    "test": "npm run lint"
  },
  "author": "DWANGO Co., Ltd.",
  "license": "MIT",
  "files": [
    "README.md",
    "package.json",
    "bin",
    "lib"
  ],
  "dependencies": {
    "@akashic/akashic-cli-commons": "0.8.0",
    "@akashic/akashic-cli-config": "0.7.0",
    "@akashic/akashic-cli-export-html": "1.4.2",
    "@akashic/akashic-cli-export-zip": "0.9.0",
    "@akashic/akashic-cli-init": "1.5.5",
    "@akashic/akashic-cli-install": "0.9.0",
    "@akashic/akashic-cli-modify": "0.7.0",
    "@akashic/akashic-cli-scan": "0.9.1",
    "@akashic/akashic-cli-serve": "1.5.5",
    "@akashic/akashic-cli-stat": "0.7.0",
    "@akashic/akashic-cli-uninstall": "0.9.0",
    "@akashic/akashic-cli-update": "0.8.0",
    "commander": "7.1.0"
  },
  "devDependencies": {
    "@types/commander": "2.12.0",
    "@types/node": "14.14.34",
    "rimraf": "3.0.2",
    "tslint": "6.1.3",
    "typescript": "3.9.9"
  },
  "publishConfig": {
    "access": "public",
    "@akashic:registry": "https://registry.npmjs.org/"
  }
}
