{
  "name": "@akashic/akashic-cli",
  "version": "2.17.25",
  "description": "Command-line utility for akashic game development",
  "type": "module",
  "bin": {
    "akashic": "./bin/akashic"
  },
  "scripts": {
    "prepublish": "npm run clean && npm run build",
    "clean": "rimraf ./lib",
    "build": "tsc -p ./",
    "lint": "eslint -c .eslintrc.cjs --fix",
    "test": "npm run lint"
  },
  "author": "DWANGO Co., Ltd.",
  "license": "MIT",
  "files": [
    "bin",
    "lib"
  ],
  "dependencies": {
    "@akashic/akashic-cli-commons": "0.15.3",
    "@akashic/akashic-cli-export": "1.9.20",
    "@akashic/akashic-cli-extra": "1.7.8",
    "@akashic/akashic-cli-init": "1.15.10",
    "@akashic/akashic-cli-lib-manage": "1.9.6",
    "@akashic/akashic-cli-sandbox": "1.1.11",
    "@akashic/akashic-cli-scan": "0.17.7",
    "@akashic/akashic-cli-serve": "1.16.18",
    "commander": "^12.0.0"
  },
  "devDependencies": {
    "@akashic/eslint-config": "^2.1.0",
    "@types/node": "^20.16.9",
    "@typescript-eslint/eslint-plugin": "^8.7.0",
    "eslint": "^8.57.1",
    "eslint-plugin-import": "^2.30.0",
    "rimraf": "^6.0.1",
    "typescript": "^5.6.2"
  },
  "publishConfig": {
    "access": "public",
    "@akashic:registry": "https://registry.npmjs.org/"
  }
}
