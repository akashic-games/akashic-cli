{
  "name": "@akashic/akashic-cli",
  "version": "1.7.1",
  "description": "Command-line utility for akashic game development",
  "bin": {
    "akashic": "./bin/akashic"
  },
  "scripts": {
    "prepublish": "npm run clean && npm run build",
    "clean": "rimraf ./lib && rimraf ./spec/build",
    "start": "npm run build && node bin/run",
    "build": "tsc -p ./",
    "lint": "tslint -c ../tslint.json --project ./tsconfig.json",
    "test": "npm run lint"
  },
  "author": "DWANGO Co., Ltd.",
  "license": "MIT",
  "files": [
    "README.md",
    "package.json",
    "bin",
    "lib"
  ],
  "dependencies": {
    "@akashic/akashic-cli-commons": "0.2.24",
    "@akashic/akashic-cli-config": "0.1.15",
    "@akashic/akashic-cli-export-html": "0.2.29",
    "@akashic/akashic-cli-export-zip": "0.3.15",
    "@akashic/akashic-cli-init": "0.4.4",
    "@akashic/akashic-cli-install": "0.3.15",
    "@akashic/akashic-cli-modify": "0.1.13",
    "@akashic/akashic-cli-scan": "0.2.16",
    "@akashic/akashic-cli-serve": "0.1.1",
    "@akashic/akashic-cli-stat": "0.1.15",
    "@akashic/akashic-cli-uninstall": "0.3.12",
    "@akashic/akashic-cli-update": "0.2.14",
    "commander": "2.8.1"
  },
  "devDependencies": {
    "@types/commander": "2.3.31",
    "@types/node": "10.12.30",
    "rimraf": "2.6.3",
    "tslint": "5.12.1",
    "typescript": "2.8.4"
  },
  "publishConfig": {
    "access": "public",
    "@akashic:registry": "https://registry.npmjs.org/"
  }
}
