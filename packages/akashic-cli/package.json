{
  "name": "@akashic/akashic-cli",
  "version": "1.14.27",
  "description": "Command-line utility for akashic game development",
  "bin": {
    "akashic": "./bin/akashic"
  },
  "scripts": {
    "prepublish": "npm run clean && npm run build",
    "clean": "rimraf ./lib && rimraf ./spec/build",
    "start": "npm run build && node bin/run",
    "build": "tsc -p ./",
    "lint": "tslint -c ../tslint.json --project ./tsconfig.json",
    "test": "npm run lint"
  },
  "author": "DWANGO Co., Ltd.",
  "license": "MIT",
  "files": [
    "README.md",
    "package.json",
    "bin",
    "lib"
  ],
  "dependencies": {
    "@akashic/akashic-cli-commons": "0.5.18",
    "@akashic/akashic-cli-config": "0.4.26",
    "@akashic/akashic-cli-export-html": "0.7.35",
    "@akashic/akashic-cli-export-zip": "0.6.28",
    "@akashic/akashic-cli-init": "0.10.46",
    "@akashic/akashic-cli-install": "0.6.28",
    "@akashic/akashic-cli-modify": "0.4.27",
    "@akashic/akashic-cli-scan": "0.5.26",
    "@akashic/akashic-cli-serve": "0.8.22",
    "@akashic/akashic-cli-stat": "0.4.27",
    "@akashic/akashic-cli-uninstall": "0.6.27",
    "@akashic/akashic-cli-update": "0.5.27",
    "commander": "5.1.0"
  },
  "devDependencies": {
    "@types/commander": "2.12.0",
    "@types/node": "10.17.27",
    "rimraf": "2.7.1",
    "tslint": "5.20.1",
    "typescript": "2.9.2"
  },
  "publishConfig": {
    "access": "public",
    "@akashic:registry": "https://registry.npmjs.org/"
  }
}
