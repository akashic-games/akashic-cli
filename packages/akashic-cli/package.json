{
  "name": "@akashic/akashic-cli",
  "version": "1.8.2",
  "description": "Command-line utility for akashic game development",
  "bin": {
    "akashic": "./bin/akashic"
  },
  "scripts": {
    "prepublish": "npm run clean && npm run build",
    "clean": "rimraf ./lib && rimraf ./spec/build",
    "start": "npm run build && node bin/run",
    "build": "tsc -p ./",
    "lint": "tslint -c ../tslint.json --project ./tsconfig.json",
    "test": "npm run lint"
  },
  "author": "DWANGO Co., Ltd.",
  "license": "MIT",
  "files": [
    "README.md",
    "package.json",
    "bin",
    "lib"
  ],
  "dependencies": {
    "@akashic/akashic-cli-commons": "0.3.0",
    "@akashic/akashic-cli-config": "0.2.0",
    "@akashic/akashic-cli-export-html": "0.3.1",
    "@akashic/akashic-cli-export-zip": "0.4.0",
    "@akashic/akashic-cli-init": "0.5.0",
    "@akashic/akashic-cli-install": "0.4.1",
    "@akashic/akashic-cli-modify": "0.2.0",
    "@akashic/akashic-cli-scan": "0.3.0",
    "@akashic/akashic-cli-serve": "0.2.0",
    "@akashic/akashic-cli-stat": "0.2.0",
    "@akashic/akashic-cli-uninstall": "0.4.0",
    "@akashic/akashic-cli-update": "0.3.0",
    "commander": "2.20.1"
  },
  "devDependencies": {
    "@types/commander": "2.12.0",
    "@types/node": "10.14.21",
    "rimraf": "2.7.1",
    "tslint": "5.20.0",
    "typescript": "2.9.2"
  },
  "publishConfig": {
    "access": "public",
    "@akashic:registry": "https://registry.npmjs.org/"
  }
}
