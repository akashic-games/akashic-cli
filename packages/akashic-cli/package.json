{
  "name": "@akashic/akashic-cli",
  "version": "1.14.58",
  "description": "Command-line utility for akashic game development",
  "bin": {
    "akashic": "./bin/akashic"
  },
  "scripts": {
    "prepublish": "npm run clean && npm run build",
    "clean": "rimraf ./lib && rimraf ./spec/build",
    "start": "npm run build && node bin/run",
    "build": "tsc -p ./",
    "lint": "tslint -c ../tslint.json --project ./tsconfig.json",
    "test": "npm run lint"
  },
  "author": "DWANGO Co., Ltd.",
  "license": "MIT",
  "files": [
    "README.md",
    "package.json",
    "bin",
    "lib"
  ],
  "dependencies": {
    "@akashic/akashic-cli-commons": "0.5.32",
    "@akashic/akashic-cli-config": "0.4.40",
    "@akashic/akashic-cli-export-html": "0.7.61",
    "@akashic/akashic-cli-export-zip": "0.6.47",
    "@akashic/akashic-cli-init": "0.10.71",
    "@akashic/akashic-cli-install": "0.6.44",
    "@akashic/akashic-cli-modify": "0.4.42",
    "@akashic/akashic-cli-scan": "0.5.42",
    "@akashic/akashic-cli-serve": "0.8.43",
    "@akashic/akashic-cli-stat": "0.4.42",
    "@akashic/akashic-cli-uninstall": "0.6.43",
    "@akashic/akashic-cli-update": "0.5.42",
    "commander": "5.1.0"
  },
  "devDependencies": {
    "@types/commander": "2.12.0",
    "@types/node": "14.6.0",
    "rimraf": "3.0.2",
    "tslint": "6.1.3",
    "typescript": "3.9.7"
  },
  "publishConfig": {
    "access": "public",
    "@akashic:registry": "https://registry.npmjs.org/"
  }
}
