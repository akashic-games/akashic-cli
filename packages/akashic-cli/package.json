{
  "name": "@akashic/akashic-cli",
  "version": "2.12.0",
  "description": "Command-line utility for akashic game development",
  "bin": {
    "akashic": "./bin/akashic"
  },
  "scripts": {
    "prepublish": "npm run clean && npm run build",
    "clean": "rimraf ./lib && rimraf ./spec/build",
    "start": "npm run build && node bin/run",
    "build": "tsc -p ./",
    "lint": "eslint \"src/**/*.ts\" --fix",
    "test": "npm run lint"
  },
  "author": "DWANGO Co., Ltd.",
  "license": "MIT",
  "files": [
    "README.md",
    "package.json",
    "bin",
    "lib"
  ],
  "dependencies": {
    "@akashic/akashic-cli-commons": "0.10.0",
    "@akashic/akashic-cli-export": "1.4.0",
    "@akashic/akashic-cli-extra": "1.4.0",
    "@akashic/akashic-cli-init": "1.10.0",
    "@akashic/akashic-cli-lib-manage": "1.4.0",
    "@akashic/akashic-cli-scan": "0.12.0",
    "@akashic/akashic-cli-serve": "1.11.0",
    "commander": "8.2.0"
  },
  "devDependencies": {
    "@akashic/eslint-config": "0.1.2",
    "@types/commander": "2.12.0",
    "@types/node": "14.17.32",
    "@typescript-eslint/eslint-plugin": "4.33.0",
    "eslint": "7.32.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-import": "2.24.2",
    "eslint-plugin-jest": "24.7.0",
    "rimraf": "3.0.2",
    "typescript": "3.9.10"
  },
  "publishConfig": {
    "access": "public",
    "@akashic:registry": "https://registry.npmjs.org/"
  }
}
