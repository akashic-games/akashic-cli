{
  "name": "@akashic/akashic-cli",
  "version": "2.17.2",
  "description": "Command-line utility for akashic game development",
  "bin": {
    "akashic": "./bin/akashic"
  },
  "scripts": {
    "prepublish": "npm run clean && npm run build",
    "clean": "rimraf ./lib && rimraf ./spec/build",
    "start": "npm run build && node bin/run",
    "build": "tsc -p ./",
    "lint": "eslint \"src/**/*.ts\" --fix",
    "test": "npm run lint"
  },
  "author": "DWANGO Co., Ltd.",
  "license": "MIT",
  "files": [
    "README.md",
    "package.json",
    "bin",
    "lib"
  ],
  "dependencies": {
    "@akashic/akashic-cli-commons": "0.15.1",
    "@akashic/akashic-cli-export": "1.9.2",
    "@akashic/akashic-cli-extra": "1.7.1",
    "@akashic/akashic-cli-init": "1.15.1",
    "@akashic/akashic-cli-lib-manage": "1.9.1",
    "@akashic/akashic-cli-sandbox": "1.1.1",
    "@akashic/akashic-cli-scan": "0.17.1",
    "@akashic/akashic-cli-serve": "1.16.2",
    "commander": "^8.3.0"
  },
  "devDependencies": {
    "@akashic/eslint-config": "^0.1.2",
    "@types/node": "^14.18.30",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^24.7.0",
    "rimraf": "^3.0.2",
    "typescript": "^3.9.10"
  },
  "publishConfig": {
    "access": "public",
    "@akashic:registry": "https://registry.npmjs.org/"
  }
}
