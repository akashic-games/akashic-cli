{
  "name": "@akashic/akashic-cli-config",
  "version": "0.6.0",
  "description": "Configuration manager for akashic.",
  "main": "lib/index.js",
  "scripts": {
    "prepublish": "npm run clean && npm run build",
    "clean": "rimraf ./lib && rimraf ./spec/build",
    "start": "npm run build && node bin/run",
    "build": "tsc -p ./",
    "lint": "eslint -c .eslintrc src/**/*.ts --fix",
    "test": "npm run test:jest && npm run lint",
    "test:jest": "jest"
  },
  "author": "DWANGO Co., Ltd.",
  "license": "MIT",
  "bin": {
    "akashic-cli-config": "./bin/run"
  },
  "files": [
    "README.md",
    "bin",
    "lib",
    "package.json"
  ],
  "devDependencies": {
    "@akashic/eslint-config": "0.1.2",
    "@types/commander": "2.12.0",
    "@types/fs-extra": "9.0.6",
    "@types/ini": "1.3.30",
    "@types/node": "14.14.20",
    "@typescript-eslint/eslint-plugin": "4.14.0",
    "eslint": "7.18.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jest": "24.1.3",
    "jest": "26.6.3",
    "mock-fs": "4.13.0",
    "rimraf": "3.0.2",
    "typescript": "3.9.7"
  },
  "typings": "lib/index.d.ts",
  "dependencies": {
    "@akashic/akashic-cli-commons": "0.7.0",
    "commander": "5.1.0",
    "fs-extra": "^9.0.0",
    "ini": "^1.3.4",
    "lodash.get": "^4.3.0",
    "lodash.set": "^4.2.0",
    "lodash.unset": "^4.4.0"
  },
