{
	"width": 320,
	"height": 320,
	"fps": 30,
	"main": "./script/main.js",
	"assets": {
		"main": {
			"type": "script",
			"path": "script/main.js",
			"global": true
		},
		"dummy": {
			"type": "audio",
			"path": "audio/dummy",
			"systemId": "sound",
			"duration": 111
		},
		"img": {
			"type": "image",
			"path": "image/img.png",
			"width": 32,
			"height": 32
		},
		"foo": {
			"type": "text",
			"path": "text/foo.txt"
		}
	},
	"renderers": ["canvas"],
	"environment": {
		"sandbox-runtime": "3",
		"nicolive": {
			"supportedModes": ["single"]
		}
	}
}
