{
  "name": "@akashic/akashic-cli-export",
  "version": "1.7.20",
  "description": "A module to convert your Akashic game to a runnable standalone.",
  "scripts": {
    "prepublish": "npm run clean && npm run build",
    "clean": "shx rm -rf lib && shx rm -rf spec/build",
    "start": "npm run build && node bin/run",
    "build": "npm run build:app && npm run build:template",
    "build:app": "tsc -p ./",
    "build:template": "shx rm -rf lib/template/ && shx cp -r src/template/ lib/",
    "update-template": "node ./script/updateEngineFiles.js",
    "lint": "npm run lint:src && npm run lint:spec && npm run lint:js",
    "lint:src": "eslint \"src/**/*.ts\" --fix",
    "lint:spec": "eslint \"./spec/src/**/*.ts\" --fix",
    "lint:js": "eslint \"./src/template/**/*.js\" --fix",
    "test": "npm run test:ts && npm run lint",
    "test:ts": "npm run test:ts:compile && npm run test:ts:jest",
    "test:ts:compile": "tsc -p ./spec",
    "test:ts:jest": "jest"
  },
  "author": "DWANGO Co., Ltd.",
  "license": "MIT",
  "bin": {
    "akashic-cli-export": "./bin/run",
    "akashic-cli-export-html": "./bin/akashic-cli-export-html",
    "akashic-cli-export-zip": "./bin/akashic-cli-export-zip",
    "akashic-cli-zip": "./bin/akashic-cli-export-zip"
  },
  "files": [
    "README.md",
    "bin",
    "lib",
    "package.json"
  ],
  "devDependencies": {
    "@akashic/akashic-engine": "~2.6.6",
    "@akashic/eslint-config": "0.1.2",
    "@types/archiver": "3.1.1",
    "@types/browserify": "12.0.37",
    "@types/commander": "2.12.0",
    "@types/ejs": "3.1.0",
    "@types/fs-extra": "9.0.13",
    "@types/fs-readdir-recursive": "1.0.0",
    "@types/jest": "26.0.24",
    "@types/mock-fs": "4.13.1",
    "@types/node": "14.18.11",
    "@types/pngjs": "~6.0.1",
    "@types/uglify-js": "3.13.1",
    "@typescript-eslint/eslint-plugin": "4.33.0",
    "browserify": "17.0.0",
    "eslint": "7.32.0",
    "eslint-plugin-import": "2.25.4",
    "eslint-plugin-jest": "24.7.0",
    "jest": "26.6.3",
    "mock-fs": "4.14.0",
    "node-fetch": "2.6.7",
    "rimraf": "3.0.2",
    "shx": "0.3.4",
    "typescript": "3.9.10",
    "uglify-save-license": "0.4.1",
    "vinyl-source-stream": "2.0.0",
    "zip": "1.2.0"
  },
  "dependencies": {
    "@akashic/akashic-cli-commons": "0.13.18",
    "@babel/core": "7.17.2",
    "@babel/preset-env": "7.16.11",
    "archiver": "5.3.0",
    "browserify": "17.0.0",
    "commander": "8.3.0",
    "ejs": "3.1.6",
    "fs-extra": "10.0.0",
    "fs-readdir-recursive": "1.1.0",
    "https": "1.0.0",
    "maxrects-packer": "~2.7.2",
    "pngjs": "~6.0.0",
    "uglify-js": "3.15.1"
  },
  "publishConfig": {
    "access": "public",
    "@akashic:registry": "https://registry.npmjs.org/"
  }
}
