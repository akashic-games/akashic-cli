{
  "name": "@akashic/akashic-cli-stat",
  "version": "0.1.9",
  "description": "Project statistics tool for akashic.",
  "main": "lib/index.js",
  "scripts": {
    "prepublish": "npm run clean && npm run build",
    "clean": "shx rm -rf lib && shx rm -rf spec/build",
    "build": "tsc -p ./",
    "lint": "tslint --type-check -c ../tslint.json --project ./tsconfig.json",
    "test": "npm run test:jasmine && npm run lint",
    "test:jasmine": "istanbul cover --report text --report html --colors -i ./lib/main.node.js ./node_modules/jasmine/bin/jasmine.js"
  },
  "author": "DWANGO Co., Ltd.",
  "license": "MIT",
  "bin": {
    "akashic-cli-stat": "./bin/run"
  },
  "files": [
    "README.md",
    "bin",
    "lib",
    "package.json"
  ],
  "devDependencies": {
    "@types/jasmine": "~2.8.2",
    "@types/mock-fs": "~3.6.30",
    "@types/node": "10.12.7",
    "istanbul": "^0.4.5",
    "jasmine": "~3.3.0",
    "jasmine-reporters": "~2.0.4",
    "jasmine-terminal-reporter": "~0.9.1",
    "mock-fs": "~4.8.0",
    "shx": "~0.2.2",
    "tslint": "~5.4.3",
    "typescript": "~2.8.0"
  },
  "typings": "lib/index.d.ts",
  "dependencies": {
    "@akashic/akashic-cli-commons": "0.2.20",
    "commander": "^2.16.0"
  },
  "publishConfig": {
    "access": "public",
    "@akashic:registry": "https://registry.npmjs.org/"
  }
}
