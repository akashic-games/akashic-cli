{
  "name": "@akashic/akashic-cli-scan",
  "version": "0.15.22",
  "description": "Command line tool for updating assets and globalScripts.",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "scripts": {
    "prepublish": "npm run clean && npm run build",
    "clean": "shx rm -rf lib && shx rm -rf spec/build",
    "build": "tsc -p ./",
    "lint": "eslint \"src/**/*.ts\" --fix",
    "test": "npm run test:build && npm run test:jasmine && npm run lint",
    "test:build": "tsc -p spec/",
    "test:jasmine": "istanbul cover --report text --report html --colors -i \"./lib/**/*\" ./node_modules/jasmine/bin/jasmine.js"
  },
  "author": "DWANGO Co., Ltd.",
  "license": "MIT",
  "bin": {
    "akashic-cli-scan": "./bin/run"
  },
  "files": [
    "bin",
    "lib"
  ],
  "devDependencies": {
    "@akashic/eslint-config": "0.1.2",
    "@akashic/game-configuration": "1.1.0-feature.0",
    "@types/image-size": "0.7.0",
    "@types/jasmine": "3.10.3",
    "@types/mock-fs": "4.13.1",
    "@types/node": "6.14.13",
    "@typescript-eslint/eslint-plugin": "4.33.0",
    "eslint": "7.32.0",
    "eslint-plugin-import": "2.25.4",
    "eslint-plugin-jest": "24.7.0",
    "istanbul": "0.4.5",
    "jasmine": "3.99.0",
    "jasmine-reporters": "2.5.0",
    "jasmine-terminal-reporter": "1.0.3",
    "mock-fs": "4.14.0",
    "shx": "0.3.4",
    "typescript": "3.9.10"
  },
  "dependencies": {
    "@akashic/akashic-cli-commons": "0.13.22",
    "aac-duration": "0.0.1",
    "chokidar": "^3.5.1",
    "commander": "^8.0.0",
    "fs-readdir-recursive": "1.1.0",
    "image-size": "~1.0.0",
    "music-metadata": "7.11.8",
    "svgson": "^5.2.1",
    "thumbcoil": "~1.2.0"
  },
  "publishConfig": {
    "access": "public",
    "@akashic:registry": "https://registry.npmjs.org/"
  }
}
