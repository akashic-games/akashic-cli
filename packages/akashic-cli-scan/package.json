{
  "name": "@akashic/akashic-cli-scan",
  "version": "1.0.0-next.0",
  "description": "Command line tool for updating assets and globalScripts.",
  "type": "module",
  "main": "lib/index.js",
  "scripts": {
    "prepublish": "npm run clean && npm run build",
    "clean": "shx rm -rf lib && shx rm -rf spec/build",
    "build": "tsc -p ./",
    "lint": "eslint -c .eslintrc.cjs --fix",
    "test": "npm run test:vitest && npm run lint",
    "test:vitest": "vitest run"
  },
  "author": "DWANGO Co., Ltd.",
  "license": "MIT",
  "bin": {
    "akashic-cli-scan": "./bin/run.js"
  },
  "files": [
    "bin",
    "lib"
  ],
  "devDependencies": {
    "@akashic/eslint-config": "2.1.0",
    "@akashic/game-configuration": "2.5.0",
    "@types/image-size": "0.8.0",
    "@types/mock-fs": "4.13.4",
    "@types/node": "18.19.44",
    "@typescript-eslint/eslint-plugin": "6.21.0",
    "eslint": "8.57.0",
    "eslint-plugin-import": "2.29.1",
    "mock-fs": "5.2.0",
    "shx": "0.3.4",
    "typescript": "5.5.4",
    "vitest": "^2.0.5"
  },
  "dependencies": {
    "@akashic/akashic-cli-commons": "1.0.0-next.0",
    "aac-duration": "0.0.1",
    "chokidar": "^4.0.0",
    "commander": "^12.0.0",
    "image-size": "~1.1.0",
    "music-metadata": "10.7.0",
    "svgson": "^5.2.1",
    "thumbcoil": "~1.2.0"
  },
  "publishConfig": {
    "access": "public",
    "@akashic:registry": "https://registry.npmjs.org/"
  }
}
