{
  "version": "independent",
  "command": {
    "publish": {
      "allowBranch": [
        "master",
        "serve-start-arguments"
      ]
    },
    "version": {
      "exact": true
    },
    "bootstrap": {
      "npmClientArgs": [
        "--no-package-lock"
      ]
    }
  },
  "packages": [
    "packages/*"
  ]
}
